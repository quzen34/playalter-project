# Backend Environment Variables for PLAYALTER
# This file inherits from main .env but can override specific backend settings

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Stripe Configuration
STRIPE_SECRET_KEY=your-stripe-secret-key-here
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key-here
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret-here

# n8n Configuration
N8N_HOST=http://localhost:5678
N8N_WEBHOOK_URL=https://playalter.app.n8n.cloud/webhook-test/webhook/stripe
N8N_SECURE_COOKIE=false

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_SECRET_KEY=playalter-secret-key-2025

# Database Configuration
DATABASE_URL=sqlite:///playalter.db

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# API Configuration
AGORA_APP_ID=your-agora-app-id
AGORA_APP_CERTIFICATE=your-agora-certificate
FACE_SWAP_API_KEY=your-face-swap-api-key
AR_MASK_API_KEY=your-ar-mask-api-key
