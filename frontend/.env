# Frontend Environment Variables for PLAYALTER
# Vite environment variables must start with VITE_

# Production Domain Configuration - UPDATED FOR www.playalter.com
VITE_DOMAIN=www.playalter.com
VITE_FRONTEND_URL=https://www.playalter.com
VITE_API_BASE_URL=https://www.playalter.com/api

# Development Configuration  
VITE_DEV_API_BASE_URL=http://localhost:8000
VITE_DEV_FRONTEND_URL=http://localhost:5173

# Stripe Configuration (Frontend) - SAFE FOR FRONTEND
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key-here

# Public API Keys (Frontend Safe)
VITE_AGORA_APP_ID=your-agora-app-id-here

# n8n Public Webhook (if needed from frontend)
VITE_N8N_WEBHOOK_URL=https://playalter.app.n8n.cloud/webhook/public

# Feature Flags
VITE_ENABLE_FACE_SWAP=true
VITE_ENABLE_AR_MASKS=true
VITE_ENABLE_LIVE_STREAM=true
VITE_ENABLE_AI_AGENTS=true

# Environment - SET TO PRODUCTION FOR www.playalter.com
VITE_NODE_ENV=production
VITE_BUILD_VERSION=2.0.0
